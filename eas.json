{
  "cli": {
    "version": ">= 14.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "android": {
        "gradleCommand": ":app:assembleDebug --warning-mode all",
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "gradleCommand": ":app:assembleDebug --warning-mode all",
        "buildType": "apk"
      },
      "prebuildCommand": "npx react-native-asset && cd android && echo 'distributionUrl=https\\://services.gradle.org/distributions/gradle-7.6.3-all.zip' > gradle/wrapper/gradle-wrapper.properties"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}